{
  "name": "overture",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "bootstrap": "pnpm install",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vue-tsc && vite build",
    "preview": "pnpm run build && vite preview",
    "preview:dist": "vite preview",
    "format": "prettier --config .prettierrc.js --write **.{js,ts,vue,less} --write vite.config.ts --no-error-on-unmatched-pattern"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "moment": "^2.30.1",
    "pinia": "^2.1.7",
    "tdesign-vue-next": "^1.9.0",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "less": "^4.2.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "style-resources-loader": "^1.5.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-style-import": "^2.0.0",
    "vue-tsc": "^1.8.27"
  },
  "env": {
    "node": true
  },
  "engines": {
    "node": ">=18"
  }
}